# Source Spaces Bucket Configuration
SOURCE_BUCKET=my-source-bucket
SOURCE_REGION=nyc3
SOURCE_ENDPOINT=https://nyc3.digitaloceanspaces.com

# Destination Spaces Bucket Configuration
DEST_BUCKET=my-backup-bucket
DEST_REGION=nyc3
DEST_ENDPOINT=https://nyc3.digitaloceanspaces.com

# DigitalOcean Spaces Credentials
# Generate these at: https://cloud.digitalocean.com/account/api/spaces
SPACES_KEY=your-spaces-access-key
SPACES_SECRET=your-spaces-secret-key

# Archive Configuration (optional)
# Prefix for the backup archive in the destination bucket
ARCHIVE_PREFIX=backups
